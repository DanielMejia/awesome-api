{
  "coveragePathIgnorePatterns": [
      "node_modules",
      "test-config",
      "interfaces",
      ".module.ts",
      "main.ts",
      "test",
      "migrations"
  ],
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "src",
  "testRegex": ".spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "../coverage",
  "testEnvironment": "node"
}